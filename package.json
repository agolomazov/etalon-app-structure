{
  "name": "@arlk_npm/arlk-tenant-ui",
  "version": "1.21.0",
  "description": "UI ЛК Арендатора Росимущества",
  "scripts": {
    "test": "jest",
    "start": "npm run clean && npm run addResources && npm run develop",
    "start:dev-stand": "cross-env DEV_STAND=dev-stand npm run start",
    "mockups": "npm run clean && npm run addResources && npm run mockups:server",
    "build": "npm run clean && npm run addResources && npm run production && npm run clear-scss",
    "build:mockups": "npm run clean && npm run addResources && npm run mockups:production && npm run clear-scss",
    "develop": "cross-env NODE_OPTIONS='--max-http-header-size=100000' webpack-dev-server --config ./webpack/app/develop",
    "mockups:server": "webpack-dev-server --config ./webpack/mockups/develop",
    "production": "webpack --config ./webpack/app/production",
    "mockups:production": "webpack --config ./webpack/mockups/production",
    "addStyles": "mkdirp ./dist/assets && ncp ./styles ./dist/assets",
    "addAssets": "ncp ./public ./dist",
    "addResources": "npm run addAssets && npm run addStyles",
    "clean": "rimraf ./dist",
    "clear-scss": "rimraf ./dist/assets/scss",
    "lint": "eslint src --color --ext .jsx,.js --fix",
    "clear-docs": "rimraf ./esdoc",
    "gen-docs": "npx esdoc",
    "open-docs": "npx http-server esdoc -o",
    "show-docs": "npm run clear-docs && npm run gen-docs && npm run open-docs",
    "cypress:open": "cypress open --config-file cypress.dev.json",
    "cypress:open:ift": "cypress open --config-file cypress.ift.json"
  },
  "repository": {
    "type": "git",
    "url": "https://atlas.swec.sbercloud.ru/bitbucket/projects/ARLK/repos/arlk-tenant-ui"
  },
  "keywords": [
    "webpack",
    "react",
    "redux",
    "redux-saga",
    "korus-ui",
    "styles-esphere"
  ],
  "author": "Anton Golomazov<Golomazov.AV@esphere.ru>",
  "license": "ISC",
